				div(class="tile-header") Total Expired 
				div(class="tile-content-wrapper")
					i(class="glyph-icon icon-cogs")					
		div(class="col-md-3")
			a(href="#", title="Example tile shortcut", class="tile-box tile-box-shortcut btn-danger")
