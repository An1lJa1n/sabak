div
	div(class="row")
		div(class="col-md-3")
			a(href="#", title="Example tile shortcut", class="tile-box tile-box-shortcut btn-primary")
				span(class="bs-badge badge-absolute") {{totalVehicals}}
				div(class="tile-header") Total Vehicles
				div(class="tile-content-wrapper")
					i(class="glyph-icon icon-dashboard")
		div(class="col-md-3")
			a(href="#", title="Example tile shortcut", class="tile-box tile-box-shortcut btn-warning")
