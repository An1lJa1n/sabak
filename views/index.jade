extends layout
block body
	#sb-site
		#loading
			.spinner
				.bounce1               
				.bounce2          
				.bounce3  
		#page-wrapper
			#page-header.bg-gradient-9
				#mobile-navigation
					button#nav-toggle.collapsed(data-toggle='collapse', data-target='#page-sidebar')
						span                    
					a.logo-content-small(href='index.html', title='MonarchUI')                
				#header-logo(style="color: white;padding-top: 5px;")
					div(style="font-size: 30px;") Sabak
					div(style="font-size: 12px;") Search | Review | Promote                         
					a#close-sidebar(href='#', title='Close sidebar')
						i.glyph-icon.icon-angle-left
				|                     
				#header-nav-left
					.user-account-btn.dropdown
						a.user-profile.clearfix(href='#', title='My Account', data-toggle='dropdown')
							img(width='28', src='../../assets/image-resources/gravatar.jpg', alt='Profile image')                                
							span Thomas Barnes
							i.glyph-icon.icon-angle-down
						.dropdown-menu.float-left
							.box-sm
								.login-box.clearfix
									.user-img
										a.change-img(href='#', title='') Change photo
										img(src='../../assets/image-resources/gravatar.jpg', alt='')
									.user-info
										span
											| Thomas Barnes
											i UX/UI developer
										a(href='#', title='Edit profile') Edit profile
										a(href='#', title='View notifications') View notifications
								.divider
								.pad5A.button-pane.button-pane-alt.text-center
									a.btn.display-block.font-normal.btn-danger(href='#')
										i.glyph-icon.icon-power-off
										|                                             Logout
				#header-nav-right
					a.hdr-btn.popover-button(href='#', title='Search', data-placement='bottom', data-id='#popover-search')
						i.glyph-icon.icon-search
					|                         
					#popover-search.hide
						.pad5A
							.input-group
								input.form-control(type='text', placeholder='Search terms here ...')
								|                                     
								span.input-group-btn
									a.btn.btn-primary(href='#') Search
					|                         
					a#fullscreen-btn.hdr-btn(href='#', title='Fullscreen')
						i.glyph-icon.icon-arrows-alt
					|                         
					#notifications-btn.dropdown
						a(data-toggle='dropdown', href='#', title='')
							span.small-badge.bg-yellow
							|                                 
							i.glyph-icon.icon-linecons-megaphone
						|                             
						.dropdown-menu.box-md.float-right
							.popover-title.display-block.clearfix.pad10A
								| Notifications
								a.text-transform-cap.font-primary.font-normal.btn-link.float-right(href='#', title='View more options')
									| More options...
							|                                 
							.scrollable-content.scrollable-slim-box
								ul.no-border.notifications-box
									li
										span.bg-danger.icon-notification.glyph-icon.icon-bullhorn
										|                                             
										span.notification-text This is an error notification
										|                                             
										.notification-time
										| a few seconds ago
										|	
										span.glyph-icon.icon-clock-o
									|                                         
									li
										span.bg-warning.icon-notification.glyph-icon.icon-users
										|                                             
										span.notification-text.font-blue This is a warning notification
										|                                             
										.notification-time
											b 15
											|  minutes ago
											|                                                 
											span.glyph-icon.icon-clock-o
							|                                 
							.pad10A.button-pane.button-pane-alt.text-center
								a.btn.btn-primary(href='#', title='View all notifications')
									| View all notifications
					|                         
					#progress-btn.dropdown
						a(data-toggle='dropdown', href='#', title='')
							span.small-badge.bg-azure
							|                                 
							i.glyph-icon.icon-linecons-params
						|                             
						#progress-dropdown.dropdown-menu.pad0A.box-sm.float-right
							.scrollable-content.scrollable-slim-box
								ul.no-border.progress-box.progress-box-links
									li
										a(href='#', title='')
											b.pull-right 23%
											|                                                 
											.progress-title
												| Finishing uploading files
											|                                                 
											.progressbar-smaller.progressbar(data-value='23')
												.progressbar-value.bg-blue-alt
													.progressbar-overlay
									|                                         
									li
										a(href='#', title='')
											b.pull-right 91%
											|                                                 
											.progress-title
												| Roadmap progress
											|                                                 
											.progressbar-smaller.progressbar(data-value='91')
												.progressbar-value.bg-red
													.progressbar-overlay
							|                                 
							.pad5A.button-pane.button-pane-alt.text-center
								a.btn.display-block.font-normal.hover-green(href='#', title='View all notifications')
									| View all notifications
					// #header-nav-right
			|                 
			#page-sidebar
				.scroll-sidebar
					ul#sidebar-menu
						li.header
							span Overview
						|                         
						li
							a(href='index.html', title='Admin Dashboard')
								i.glyph-icon.icon-linecons-tv
								|                                 
								span Dashboard
						|                         
						li.divider
						|                         
						li.header
							span Components
						|                         
						li
							a(ng-href='vehicalServices', title='Dashboard boxes')
								i.glyph-icon.icon-linecons-lightbulb
								|                                 
								span Vehicals
								|                                 
								span.bs-label.label-primary Hot
						|                         
						li
							a(ng-href='driverServices', title='Widgets')
								i.glyph-icon.icon-linecons-wallet
								|                                 
								span Drivers
						|                         
						li.header
							span Extra
						|                         
						li
							a(href='#', title='Helpers')
								i.glyph-icon.icon-linecons-doc
								|                                 
								span Helpers
							|                             
							.sidebar-submenu
								ul
									li
										a(href='helper-classes.html', title='Helper classes')
											span Helper classes
									|                                     
									li
										a(href='page-transitions.html', title='Page transitions')
											span Page transitions
									|                                     
									li
										a(href='animations.html', title='Animations')
											span Animations
							// .sidebar-submenu
					// #sidebar-menu
			|                 
			#page-content-wrapper
				#page-content
					.container
						#page-title
							div(ng-view, style="font-size:small")
								ul
									li(ng-repeat="event in events track by $index", ng-bind="event")
		|             
		script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js")
		script(type='text/javascript', src='js/admin-all-demo.js')
		script(src='https://ajax.googleapis.com/ajax/libs/angularjs/1.4.7/angular.js')
		script(src='https://ajax.googleapis.com/ajax/libs/angularjs/1.4.7/angular-route.js')
		script(src="http://ajax.googleapis.com/ajax/libs/angularjs/1.4.7/angular-animate.js")
		script(src="http://wuxiaoying.github.io/angular-multilevelpushmenu/javascripts/pushmenu.js")
		script(src="http://angular-ui.github.io/bootstrap/ui-bootstrap-tpls-0.14.3.js")
		script(src="https://cdnjs.cloudflare.com/ajax/libs/lodash.js/3.10.0/lodash.js")
		script(src="http://ajax.googleapis.com/ajax/libs/angularjs/1.4.7/angular-messages.js")
		script(src="js/lib/smart-table.js")
		script(src="js/lib/ng-file-upload.min.js")
		script(src="js/lib/ng-file-upload-shim.min.js")
		script(src="js/lib/angular-file-saver.bundle.min.js")
		script(src='js/app.js')
		script(src='js/services.js')
		script(src='js/controllers.js')
		script(src='js/filters.js')
		script(src='js/directives.js')
		script(src="http://cdn.jsdelivr.net/alasql/0.2/alasql.min.js")
